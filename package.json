{
  "name": "excalidraw",
  "main": "./lib/excalidraw.js",
  "version": "0.2.0",
  "description": "Allows Inkdrop to embed Excalidraw contents to a note. ",
  "keywords": [
    "inkdrop"
  ],
  "repository": "https://github.com/azu/inkdrop-excalidraw",
  "license": "MIT",
  "engines": {
    "inkdrop": "^4.x"
  },
  "files": [
    "lib",
    "menus",
    "keymaps",
    "docs",
    "resources"
  ],
  "scripts": {
    "build": "babel src/ -d lib/",
    "watch": "babel src/ -d lib/ --watch",
    "dev": "babel src/ -s -d lib/ --watch",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepublish": "npm run build",
    "postinstall": "cp -r node_modules/@excalidraw/excalidraw/dist/ resources/"
  },
  "dependencies": {
    "@excalidraw/excalidraw": "^0.7.0",
    "dayjs": "^1.10.4",
    "prop-types": "^15.7.2",
    "use-debounce": "^6.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/compat-data": "^7.14.0",
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-react": "^7.13.13",
    "babel-eslint": "^10.1.0",
    "electron": "^12.0.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "prettier": "^2.2.1"
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 4,
    "trailingComma": "none"
  }
}
